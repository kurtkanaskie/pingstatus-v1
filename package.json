    "integration": "node ./node_modules/cucumber/bin/cucumber.js target/test/integration/features"
  },
  "devDependencies": {
    "apickli": "^1.7.0",
    "apigeelint": "0.4.6",
    "bluebird": "^3.3.5",
    "chai": "^3",
    "chai-fuzzy": "^1",
    "cucumber": "^1.0.0",
    "mocha": "^8.1.1",
    "prettyjson": "^1.1.3",
    "sinon": "^1"
  },
  "jshintConfig": {
    "predef": [
      "console",
      "context"
    ],
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "undef": true,
    "latedef": true,
    "noempty": true,
    "nonew": true,
    "unused": true
  },
  "dependencies": {
    "tmp": "^0.1.0"
